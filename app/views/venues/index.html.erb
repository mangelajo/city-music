<p id="notice"><%= notice %></p>

<h1>Venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Street</th>
      <th>Postalcode</th>
      <th>Phone</th>
      <th>Description</th>
      <th>Website</th>
      <th>Rating</th>
      <th>Capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.name %></td>
        <td><%= venue.city_id %></td>
        <td><%= venue.street %></td>
        <td><%= venue.postalCode %></td>
        <td><%= venue.phone %></td>
        <td><%= venue.description %></td>
        <td><%= venue.website %></td>
        <td><%= venue.rating %></td>
        <td><%= venue.capacity %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
